    <test name="Web - Star+ - Smoke Tests - User" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.olpSignup.DisneyPlusOlpSignupTest">
                <methods>
                    <exclude name="welcomePageReview"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Star+ - Smoke Tests - Profile" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfileTest">
                <methods>
                    <exclude name="verifySetProfilePin"/>
                    <exclude name="verifyCreateProfileFlow"/>
                    <exclude name="verifyJuniorProfileSet"/>
                    <exclude name="verifyCharacterCategoryTiles"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Star+ - Smoke Tests - Monthly Subscription" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceCreditCardTest">
                <methods>
                    <include name="monthly"/>
                    <include name="superBundleSash"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Star+ - Smoke Tests - Account" parallel="methods">
