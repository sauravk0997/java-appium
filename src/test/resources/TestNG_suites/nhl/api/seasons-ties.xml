<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite verbose="1" name="API - NHL - Seasons Ties .api" annotations="JDK">

    <parameter name="suiteOwner" value="shashem"/>
    <parameter name="reporting.project-key" value="NHL" />
    <parameter name="jenkinsJobType" value="api"/>
    <parameter name="jenkinsJobName" value="NHL-API-SeasonTies"/>
    <parameter name="jenkinsUrl" value="http://statsapi.web.nhl.com/api/v1/gameStatus"/>
    <parameter name="jenkinsEmail" value="pace@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="PROD,QA,DEV"/>
    <parameter name="jenkinsRegressionPipeline" value="NHL-API-Regression-Pipeline"/>
    <parameter name="jenkinsRegressionScheduling" value="@daily"/>

    <test verbose="1" name="NHL Seasons Ties Test" enabled="true">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.nhl.api.NhlSeasonsTiesTest"/>
        </classes>
    </test>

</suite>