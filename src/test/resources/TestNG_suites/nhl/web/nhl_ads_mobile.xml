<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Web - NHL - Ad Tests"
       parallel="methods"
       data-provider-thread-count="1"
       annotations="JDK">
    <parameter name="suiteOwner" value="msantera"/>
    <parameter name="reporting.project-key" value="NHL" />
    <parameter name="TestRailSuiteId" value="14843"/>
    <parameter name="TestRailProjectId" value="74"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsJobName" value="NHL-Web-Ads-Mobile"/>
    <parameter name="jenkinsUrl" value="http://www.nhl.com"/>
    <parameter name="jenkinsEmail" value="WebQA@disneystreaming.com"/>

    <test verbose="1" name="NHL - Web - Ad tests - Mobile">
        <groups>
            <run>
                <include name="Mobile"/>
            </run>
        </groups>
        <classes>
            <class name="com.disney.qa.tests.nhl.web.NhlAdsTest"/>
        </classes>
    </test>

</suite>