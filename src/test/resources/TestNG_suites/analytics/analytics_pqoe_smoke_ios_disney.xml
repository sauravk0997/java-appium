<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Analytics Test Runs iOS" annotations="JDK" thread-count="1" configfailurepolicy="continue">

    <parameter name="suiteOwner" value="isong1"/>
    <parameter name="reporting.project-key" value="ANA01"/>
    <parameter name="hiddenParam::appcenter_app_name::app center app name" value="Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="DisneyPlus-Analytics-iOS-Run"/>
    <parameter name="jenkinsEmail" value="insung.song@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="Prod,QA"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=ANY;deviceType=Tablet;timeoutMs=120000"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value="DisneyPlusQoETest"/>
    <parameter name="jenkinsDefaultRetryCount" value="1"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="overrideFields" value=""/>

    <test verbose="1" name="DisneyPlus iOS Handset Smoke PQoE" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.qoeplayback.DisneyPlusQoETest"/>
        </classes>
    </test>
</suite>
