<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Analytics Test Runs" annotations="JDK" thread-count="1" configfailurepolicy="continue">

    <parameter name="suiteOwner" value="scho2"/>
    <parameter name="reporting.project-key" value="ANA01"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsJobName" value="DisneyPlus-Analytics-Web-Run"/>
    <parameter name="reporting.tcm.xray.test-execution-key" value="XAQA-1530"/>
    <parameter name="jenkinsEnvironments" value="PROD"/>
    <parameter name="jenkinsUrl" value="https://www.disneyplus.com/"/>
    <parameter name="jenkinsEnvironments" value="PROD,QA,BETA,PREVIEW"/>
    <parameter name="stringParam::app_version::Branch to test" value="MAIN"/>
    <parameter name="choiceParam::locale::Locale to run" value="US,GB,ES,CA,NL"/>
    <parameter name="choiceParam::language::Language set for Accounts" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="choiceParam::browser_language::Browser Language to run" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="choiceParam::groups::Group to run" value="US,AG,AI,AR,AT,AU,AW,BB,BE,BL,BM,BO,BQ,BR,BS,BZ,CA,CH,CL,CO,CR,CR,CW,DE,DK,DK,DM,DO,EC,ES,FI,FK,FR,GB,GD,GF,GG,GP,GR,GS,GT,GY,HK,HN,HT,IE,IM,IS,IT,JE,JM,JP,KR,KN,KY,LC,LU,MC,ME,MF,MQ,MS,MU,MX,NC,NI,NL,NO,NO,NZ,PA,PE,PR,PT,PY,RE,RU,SE,SG,SR,SV,TC,TT,TW,UY,VC,VE,VG,VI,WF,YT"/>
    <parameter name="jenkinsDefaultRetryCount" value="1"/>
    <parameter name="overrideFields" value="test_naming_pattern={method_description},thread_count=20,browsermob_proxy=true,retry_interval=750,node_label=2xlarge,enable_hora_validation=true"/>
    <parameter name="jenkinsEmail" value="susie.cho@disneystreaming.com,dan.cordell@disneystreaming.com,jeff.demelle@disneystreaming.com,c-brien.perkins@disneystreaming.com,ben.newman@disneystreaming.com"/>
    <parameter name="jenkinsRegressionScheduling" value="0 6 * * *"/>
    <parameter name="jenkinsRegressionPipeline" value="DisneyPlus-Analytics-Web-Pipeline"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="jenkinsQueueRegistration" value="false"/>

    <test name="Web - Disney+ - Smoke Tests - User" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.olpSignup.DisneyPlusOlpSignupTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Profile" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfileTest">
                <methods>
                    <exclude name="verifySetProfilePinMX"/>
                    <exclude name="verifyCreateProfileFlowMX"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Account" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.account.DisneyPlusProfileAccountManagementTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Homepage" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.homepage.DisneyPlusHomePageCarouselTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Originals" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.originals.DisneyPlusOriginalsTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Movies" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.movies.DisneyPlusMoviesTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Watchlist" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.watchlist.DisneyPlusWatchlistTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Series" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.series.DisneyPlusSeriesTest">
                <methods>
                    <exclude name="verifyShopDisneyProfiles"/>
                    <exclude name="verifyShopDisneyTab"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Playback" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.playback.DisneyPlusPlaybackTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Vanity Redirect" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.redirects.DisneyPlusVanityRedirectsTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Smoke Tests - Playback Options Test" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.playback.DisneyPlusPlaybackOptionsTest"/>
        </classes>
    </test>


</suite>