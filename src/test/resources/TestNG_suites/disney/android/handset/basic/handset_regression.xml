<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" thread-count="8" name="Android - Disney+ - Handset Regression" annotations="JDK">

	<parameter name="suiteOwner" value="bsuscavage"/>
	<parameter name="reporting.project-key" value="DIS"/>
	<parameter name="reporting.notification.slack-channels" value="android-mobile-zebrunner-reports"/>
	<parameter name="applicationPackage" value="com.disney.disneyplus"/>
	<parameter name="jenkinsJobType" value="android"/>
	<parameter name="jenkinsJobName" value="DisneyPlus-Android-HandSet-Regression"/>
	<parameter name="jenkinsEmail"
			   value="mobileqa@disneystreaming.com,nathan.lefler@disneystreaming.com,jennifer.yee@disneystreaming.com"/>
	<parameter name="jenkinsRegressionPipeline" value="Disney-Android-Regression-Pipeline"/>
	<parameter name="choiceParam::appcenter_app_name::AppCenter App Name" value="Disney-Mobile-Prod-Google-Debug,Disney-Mobile-Prod-Google-Release"/>
	<parameter name="jenkinsEnvironments" value="mobile.google"/>
	<parameter name="jenkinsMobileDefaultPool" value="any"/>
	<parameter name="jenkinsRegressionScheduling" value="H 3 * * *"/>
	<parameter name="jenkinsFetchAppFrom" value="appcenter"/>
	<parameter name="jenkinsRegressionMatrix" value="locale:US, language:en"/>
	<parameter name="jenkinsDefaultRetryCount" value="1"/>
	<parameter name="overrideFields"
			   value="capabilities.deviceType=Phone,capabilities.locale=${locale},capabilities.language=${language},capabilities.custom_env=${custom_env},custom_string=${countries},JOB_MAX_RUN_TIME=600,capabilities.androidInstallTimeout=120000,capabilities.adbExecTimeout=90000,capabilities.uiautomator2ServerInstallTimeout=120000"/>
	<parameter name="choiceParam::custom_env::App Environment" value="PROD,QA,EDITORIAL"/>
	<parameter name="choiceParam::locale::Locale to run. Blank default is US." value=",US,GB,ES,CA,NL,IT,DE,FR,AU,NZ,JP,BR,KR"/>
	<parameter name="choiceParam::language::Language to run. Blank default is en." value=",da,de,en,es,fi,fr,it,ja,ko,nl,no,pt,sk,sv"/>
	<parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression."
			   value=",Smoke,ArielLogIn,ArielSignUp,Deeplinks,Downloads,Home,IAP,Log In,More Menu,Navigation,Onboarding,Profiles,Ratings,Search,Upgrade,Video Player,Welch,Sign Up"/>
	<parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
	<parameter name="choiceParam::appcenter_use_jenkins_build_version::Override hardcoded build version" value="false,true"/>

