<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" thread-count="8" name="Android - Disney+ - Handset Regression Ratings" annotations="JDK">

	<parameter name="suiteOwner" value="bsuscavage"/>
	<parameter name="reporting.project-key" value="DIS"/>
	<parameter name="reporting.notification.slack-channels" value="android-mobile-zebrunner-reports"/>
	<parameter name="applicationPackage" value="com.disney.disneyplus"/>
	<parameter name="jenkinsJobType" value="android"/>
	<parameter name="jenkinsJobName" value="DisneyPlus-Android-HandSet-Regression-Ratings"/>
	<parameter name="jenkinsEmail" value="mobileqa@disneystreaming.com,nathan.lefler@disneystreaming.com,jennifer.yee@disneystreaming.com"/>
	<parameter name="choiceParam::appcenter_app_name::AppCenter App Name" value="Disney-Mobile-Prod-Google-Debug,Disney-Mobile-Prod-Google-Release"/>
	<parameter name="jenkinsEnvironments" value="mobile.google"/>
	<parameter name="jenkinsMobileDefaultPool" value="any"/>
	<parameter name="scheduling" value="H 12 * * 6"/>
	<parameter name="jenkinsFetchAppFrom" value="appcenter"/>
	<parameter name="jenkinsDefaultRetryCount" value="2"/>
	<parameter name="overrideFields"
			   value="capabilities.deviceType=Phone,capabilities.locale=${locale},capabilities.language=${language},capabilities.custom_env=${custom_env},custom_string=${countries},JOB_MAX_RUN_TIME=600,capabilities.androidInstallTimeout=120000,capabilities.adbExecTimeout=90000,capabilities.uiautomator2ServerInstallTimeout=120000,enable_ratings_configuration=true"/>
	<parameter name="choiceParam::custom_env::App Environment" value="PROD,QA,EDITORIAL"/>
	<parameter name="choiceParam::locale::Locale to run" value="US,GB,ES,CA,NL,IT,DE,FR,AU,NZ,JP,KR"/>
	<parameter name="choiceParam::language::Language to run" value="en,es,fr,nl,it,de,ja,ko"/>
	<parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
	<parameter name="choiceParam::appcenter_use_jenkins_build_version::Override hardcoded build version" value="false,true"/>

	<test verbose="1" parallel="methods" name="DisneyPlus DJCTQ Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Brazil"/>
		<parameter name="ratings" value="L,10,12,14,16,18"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestDJCTQ"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus MPAA/TVPG Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Canada,United States"/>
		<parameter name="ratings" value="TV-14,TV-G,TV-MA,TV-PG,TV-Y,TV-Y7,TV-Y7-FV,G,NC-17,PG,PG-13,R"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestMPAAandTVPG"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus E-FSF-FSK Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Austria,Germany"/>
		<parameter name="ratings" value="0,6,12,16,18"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestFSKandFSFandE"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus EMEA Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Belgium,Denmark,Finland,France,Greenland,Haiti,Iceland,Ireland,Italy,Luxembourg,Mauritius,Mayotte,Norway,Portugal,Reunion,Spain,Sweden,Switzerland,United Kingdom"/>
		<parameter name="ratings" value="0,6,9,12,14,16,18"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestEMEA"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus Kijkwijzer Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Netherlands"/>
		<parameter name="ratings" value="AL,6,9,12,16,18"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestKIJKWIJZER"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus LATAM Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Argentina,Bolivia,Chile,Colombia,Costa Rica,Dominican Republic,Ecuador,El Salvador,Guatemala,Honduras,Mexico,Nicaragua,Panama,Paraguay,Peru,Uruguay"/>
		<parameter name="ratings" value="0+,7+,10+,12+,13+,14+,16+,18+"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestLATAM"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus MDA Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Singapore"/>
		<parameter name="ratings" value="G,PG,PG13,NC16,M18,R21"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestMDA"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus NCS Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="Australia"/>
		<parameter name="ratings" value="G,PG,M,MA15,R18+"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestNCS"/>
		</classes>
	</test>

	<test verbose="1" parallel="methods" name="DisneyPlus OFLC Ratings Acceptance" >
		<parameter name="{ds_custom_provider}" value="true"/>
		<parameter name="countries" value="New Zealand"/>
		<parameter name="ratings" value="G,M,PG,R15,R16,RP13,RP16,R,R18,18,RP18"/>
		<classes>
			<class name="com.disney.qa.tests.disney.android.mobile.ratings.DisneyPlusRatingsTestOFLC"/>
		</classes>
	</test>

</suite>
