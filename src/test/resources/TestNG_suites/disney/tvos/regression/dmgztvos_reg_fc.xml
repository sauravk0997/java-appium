<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="dmgztvos_reg_fc" annotations="JDK" configfailurepolicy="continue" thread-count="4" parallel="tests">
    <parameter name="suiteOwner" value="apple-automation"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="stringParam::reporting.milestone.name::Enter Milestone Name" value="apple-3.7.0-fc"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value="XMOBQA-77304"/>
    <parameter name="jenkinsBuild" value="3.7.0.72769"/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="dmgztvos_reg_fc"/>
    <parameter name="jenkinsEnvironments" value="PROD"/>
    <parameter name="jenkinsEmail" value="eric.liao@disney.com,vaibhavi.zaveri@disney.com"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="deviceName=ATV_4_Gen_12,ATV_4_Gen_2;platformName=iOS;deviceType=tvOS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Non-IAP-Prod-Enterprise-tvOS,Dominguez-Non-IAP-Prod-Enterprise-for-Automation-tvOS"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression." value=",Smoke,Onboarding,Home,Details,Watchlist,Profile"/>
    <parameter name="scheduling" value="H 12 * * 1-5"/>
    <parameter name="overrideFields" value="init_retry_count=0,init_retry_interval=15,capabilities.newCommandTimeout=240"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>

    <test verbose="1" name="DisneyPlus tvOS Basic" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.basic.DisneyPlusAppleTVAppLaunchTest">
            </class>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Details" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.details.DisneyPlusAppleTVDetailsScreenTests"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Home" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.home.DisneyPlusAppleTVGlobalNavMenuTest"/>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.home.DisneyPlusAppleTVHomeTests"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Onboarding" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.onboarding.DisneyPlusAppleTVForgotPasswordTests"/>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.onboarding.DisneyPlusAppleTVLoginTests"/>
<!--            <class name="com.disney.qa.tests.disney.apple.tvos.regression.onboarding.DisneyPlusAppleTVSignUpTest"/>-->
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.onboarding.DisneyPlusAppleTVWelcomeScreenTests"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Originals" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.originals.DisneyPlusAppleTVOriginalsTest">
            </class>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Settings" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.settings.DisneyPlusAppleTVProfilesTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Watchlist" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.watchlist.DisneyPlusAppleTVWatchlistTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus tvOS Anthology" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.anthology.DisneyPlusAppleTVAnthologyTest"/>
        </classes>
    </test>
</suite>
