<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="tvOS_alice_demo_nightly" annotations="JDK" thread-count="5" configfailurepolicy="continue">

    <parameter name="suiteOwner" value="fkhan"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="tvOS_alice_demo_nightly"/>
    <parameter name="capabilities"
               value="deviceName=ANY;platformName=iOS;deviceType=tvOS"/>
    <parameter name="jenkinsEnvironments" value="PROD"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="stringParam::appcenter_app_name::app center app name" value="Dominguez-Non-IAP-Prod-Enterprise-for-Automation-tvOS"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>

    <parameter name="overrideFields" value="init_retry_count=2,init_retry_interval=15"/>

    <test verbose="1" name="DisneyPlus tvOS Alice" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.tvos.regression.alice.DisneyPlusAppleTVAlice">
                <methods>
                    <include name="testAppLaunch"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>