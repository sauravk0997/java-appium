<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Web - Disney+ - AppExRegression Test - Suite" parallel="tests" thread-count="10">

    <parameter name="suiteOwner" value="Web QAA"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsJobName" value="Disney-Plus-Web-AppEx-Regression-Test"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::XRay test execution value" value="XWEBQAS-31173"/>
    <parameter name="jenkinsUrl" value="https://www.disneyplus.com/"/>
    <parameter name="jenkinsEmail" value="web-qaa-team@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="PROD,QA,BETA,PREVIEW"/>
    <parameter name="jenkinsQueueRegistration" value="false"/>
    <parameter name="jenkinsRegressionPipeline" value="Disney-Plus-Web-AppEx-Regression-Pipeline"/>
    <parameter name="jenkinsRegressionScheduling" value="@daily"/>
    <parameter name="jenkinsRegressionMatrix_Disney-Plus-Web-AppEx-Regression-Test-US" value="browser:chrome, locale:US, language:en"/>
    <parameter name="hiddenParam::reporting.notification.slack-channels::custom slack notification channels" value="web-dss-nightly-health" />
    <parameter name="stringParam::app_version::Branch to test" value="MAIN"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="choiceParam::locale::Locale to run" value="US,AG,AI,AR,AT,AU,AW,BB,BE,BL,BM,BO,BQ,BR,BS,BZ,CA,CH,CL,CO,CR,CR,CW,DE,DK,DK,DM,DO,EC,ES,FI,FK,FR,GB,GD,GF,GG,GP,GR,GS,GT,GY,HK,HN,HT,IE,IM,IS,IT,JE,JM,JP,KR,KN,KY,LC,LU,MC,ME,MF,MQ,MS,MU,MX,NC,NI,NL,NO,NO,NZ,PA,PE,PR,PT,PY,RE,RU,SE,SG,SR,SV,TC,TT,TW,UY,VC,VE,VG,VI,WF,YT"/>
    <parameter name="choiceParam::mapLocaleToGroups::set to true to map" value="false,true"/>
    <parameter name="choiceParam::language::Language set for Accounts" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="choiceParam::browser_language::Browser Language to run" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="booleanParam::proxy_server_geoedge::Request GeoEdge API for IP Address instead of using local proxy-server.yaml file" value="true"/>
    <parameter name="jenkinsDefaultRetryCount" value="2"/>
    <parameter name="overrideFields" value="test_naming_pattern={method_description},thread_count=20,browsermob_proxy=true,retry_interval=750,node_label=mitm"/>
    <parameter name="stringParam::git_hash::Git hash commit value" value=""/>
    <parameter name="choiceParam::groups::Test Group selection disney_appex." value="disney_appex,disney_smoke,web_home,profiles,search,ariel_appex,hubs,groupwatch,player_ads"/>
    <parameter name="maxNumberKeepBuilds" value="200"/>

    <test name="Web - Disney+ - Regression Tests - Account" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.account.DisneyPlusProfileAccountManagementTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - GlobalNav" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.globalNav.DisneyPlusNavAccountMenuTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.globalNav.DisneyPlusNavProfileSwitcherTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.globalNav.DisneyPlusNavStateHoverTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.globalNav.DisneyPlusNavStateNavLinksTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Home" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.homepage.DisneyPlusHomePageCarouselTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Movies" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.movies.DisneyPlusMoviesTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Signup" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.olpSignup.DisneyPlusOlpSignupTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.olpSignup.DisneyPlusUserFlowEmailViewTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.olpSignup.DisneyPlusUserFlowPasswordViewTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Originals" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.originals.DisneyPlusOriginalsTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Playback" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.playback.DisneyPlusPlaybackTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.playback.DisneyPlusPlaybackOptionsTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.playback.DisneyPlusPlaybackAdsTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Profile" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusAddProfileTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusCreateProfileTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusDefaultProfileTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfilePickerTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfileSelectionViewTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusSetUpProfilesTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusSetUpProfileViewTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusAutoplayTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusDeleteProfileTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusEditNamePageTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusEditProfileTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusJuniorModeTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfileTest">
                <methods>
                    <exclude name="verifySetProfilePinMX"/>
                    <exclude name="verifyCreateProfileFlowMX"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Purchase" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.purchase.DisneyPlusPurchaseTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Redirects" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.redirects.DisneyPlusVanityRedirectsTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Search" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.search.DisneyPlusSearchTest"/>
            <class name="com.disney.qa.tests.disney.web.appex.search.DisneyPlusNewSearchTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Series" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.series.DisneyPlusSeriesTest">
                <methods>
                    <exclude name="verifyDetailsPageForLionsGateEntitledUser"/>
                    <exclude name="verifyDetailsPageForLionsGateUnentitledUser"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Watchlist" parallel="methods">
        <parameter name="{ds_custom_provider}" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.watchlist.DisneyPlusWatchlistTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Regression Tests - Hubs" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.hubs.DisneyPlusBrandTilesTest">
                <methods>
                    <exclude name="verifyLionsGateBrandPage"/>
                    <exclude name="verifyTileImageBadgingForLionsGate"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
