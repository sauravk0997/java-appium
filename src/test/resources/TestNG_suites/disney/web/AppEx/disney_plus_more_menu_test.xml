    <test name="Web - Disney+ - More - Menu - Edit Profile - Kids Mode - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusJuniorModeTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Edit Profiles - Edit Profile - Tests" parallel="methods">
