<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite verbose="1" name= "Web - Disney+ - More Menu - Suite" parallel="tests" thread-count="50">

    <parameter name="suiteOwner" value="Web QAA"/>
    <parameter name="reporting.project-key" value="DIS" />
    <parameter name="reporting.notification.slack-channels" value="dplus-web-automation-zebrunner"/>
    <parameter name="com.zebrunner.app/tcm.qtest.project-id" value="19" />
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsJobName" value="Disney-Plus-Web-More-Menu-Test"/>
    <parameter name="jenkinsUrl" value="https://www.disneyplus.com"/>
    <parameter name="jenkinsEmail" value="webqa@disneystreaming.com,derek.smith@disneystreaming.com, web-qaa-team@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="PROD,QA,BETA,PREVIEW"/>
    <parameter name="jenkinsRegressionPipeline" value="Disney-Plus-Web-More-Menu-Pipeline-US-East-1,Disney-Plus-Web-More-Menu-Pipeline-EU-West-1"/>
    <parameter name="jenkinsRegressionScheduling" value="@daily"/>
    <parameter name="jenkinsQueueRegistration" value="false"/>
    <parameter name="jenkinsRegressionMatrix_Disney-Plus-Web-More-Menu-Pipeline-US-East-1" value="browser:chrome, locale:US, language:en; browser:firefox, locale:US, language:en"/>
    <parameter name="stringParam::app_version::Branch to test" value="MAIN"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="choiceParam::locale::Locale to run" value="US,GB,ES,CA,NL"/>
    <parameter name="choiceParam::language::Language set for Accounts" value="en,en-GB,es-ES,fr-CA,nl"/>
    <parameter name="booleanParam::proxy_server_geoedge::Request GeoEdge API for IP Address instead of using local proxy-server.yaml file" value="true"/>
    <parameter name="jenkinsDefaultRetryCount" value="2"/>
    <parameter name="jenkinsAutoScreenshot" value="true"/>
    <parameter name="overrideFields" value="test_naming_pattern={method_description},thread_count=20,browsermob_proxy=true,node_label=4xlarge,retry_interval=750"/>

    <test name="Web - Disney+ - More - Menu - Default Profile - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusDefaultProfileTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Profile Picker - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfilePickerTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Create Profile - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusCreateProfileTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Add Profile - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusAddProfileTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Profile Selection - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.DisneyPlusProfileSelectionViewTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Edit Profile - Edit Name - Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.appex.profile.editProfile.DisneyPlusEditNamePageTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - More - Menu - Edit Profile - Delete Profile - Tests" parallel="methods">
