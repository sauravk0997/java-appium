<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite verbose="1" name= "Web - Disney+ - Commerce - MITM" parallel="tests" thread-count="50">

    <parameter name="suiteOwner" value="Web QAA"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::XRay test execution value" value=""/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsJobName" value="Disney-Plus-Web-Commerce--Mitm-Test"/>
    <parameter name="jenkinsUrl" value="https://disneyplus.com/"/>
    <parameter name="jenkinsEmail" value="web-qaa-team@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="QA,PROD,BETA,PREVIEW"/>
    <parameter name="jenkinsQueueRegistration" value="false"/>
    <parameter name="jenkinsRegressionScheduling" value="@daily"/>
    <parameter name="stringParam::app_version::Branch to test" value="main"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="choiceParam::locale::Locale to run" value="US,AD,AG,AI,AL,AR,AS,AT,AU,AW,AX,BA,BB,BE,BG,BL,BM,BO,BQ,BR,BS,BZ,CA,CC,CH,CK,CL,CO,CR,CR,CW,CX,CZ,DE,DK,DK,DM,DO,EC,EE,ES,FI,FK,FO,FR,GB,GD,GF,GG,GI,GP,GR,GR,GS,GT,GU,GY,HK,HN,HR,HT,HU,IE,IM,IO,IS,IT,JE,JM,JP,KN,KR,KY,LC,LI,LT,LU,LV,MC,ME,ME,MF,MH,MK,MP,MQ,MS,MT,MU,MX,NC,NF,NI,NL,NO,NU,NZ,PA,PE,PF,PL,PM,PN,PR,PT,PY,RE,RO,RS,RU,SE,SG,SH,SI,SI,SJ,SK,SK,SM,SR,SV,SX,TC,TF,TK,TR,TT,TW,UM,UY,VA,VC,VE,VG,VI,WF,YT"/>
    <parameter name="mapLocaleToGroups" value="true"/>
    <parameter name="choiceParam::language::Language set for Accounts" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="choiceParam::browser_language::Browser Language to run" value="en,en-GB,cs,da,de,el,es-ES,es-419,fi,fr,fr-CA,hu,it,ja,ko,nl,no,pl,pt-PT,pt-BR,ro,sk,sv,tr,zh-hans,zh-hant,zh-HK"/>
    <parameter name="booleanParam::proxy_har_capture_all::Har Capture all tests" value="false" />
    <parameter name="booleanParam::proxy_har_capture_on_failure::Har Capture only failures" value="false" />
    <parameter name="booleanParam::proxy_server_geoedge::Request GeoEdge API for IP Address instead of using local proxy-server.yaml file" value="true"/>
    <parameter name="jenkinsDefaultRetryCount" value="2"/>
    <parameter name="overrideFields" value="reporting.project-key=DIS,test_naming_pattern={method_description},browsermob_proxy=true,retry_interval=750,node_label=mitm,enable_fuzzbucket_scaling=true"/>
    <parameter name="stringParam::git_hash::Git hash commit value" value=""/>
    <parameter name="maxNumberKeepBuilds" value="200"/>

    <test name="Web - Disney+ - Commerce - Plan Switch" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.planswitch.DisneyPlusCommercePlanSwitchTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Cancel and Save" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.cancel.DisneyPlusCommerceCancelTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Billing history" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.account.DisneyPlusCommerceBillingHistoryTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Mobile Billing history" parallel="methods">
        <parameter name="isMobileUserAgent" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.account.DisneyPlusCommerceBillingHistoryTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Upgrade Bundle Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.upgrades.DisneyPlusCommerceUnifiedUpgradeBundleTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Early Access Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.earlyAccess.DisneyPlusCommerceEarlyAccess"/>
            <methods>
                <include name="eaEntitledHasEAInvoiceTwoCheck"/>
            </methods>
        </classes>
    </test>

</suite>
