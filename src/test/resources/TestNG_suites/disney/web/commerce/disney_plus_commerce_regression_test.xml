    <test name="Web - Disney+ - Commerce - Purchase Tests" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceKlarnaTest"/>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceIdealTest"/>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommercePayPalTest"/>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceMercadoPagoTest"/>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceCreditCardTest"/>
        </classes>
    </test>

    <test name="Web - Disney+ - Commerce - Mobile Purchase Tests" parallel="methods">
        <parameter name="isMobileUserAgent" value="true"/>
        <classes>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceKlarnaTest">
                <methods>
                    <exclude name="annual"/>
                    <exclude name="monthlyKlarnaUnavailable"/>
                </methods>
            </class>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceIdealTest">
                <methods>
                    <exclude name="annual"/>
                    <exclude name="monthlyFailure"/>
                </methods>
            </class>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommercePayPalTest">
                <methods>
                    <exclude name="annual"/>
                    <exclude name="superBundleSash"/>
                    <exclude name="paymentBlocked"/>
                    <exclude name="deepLink"/>
                </methods>
            </class>
            <class name="com.disney.qa.tests.disney.web.commerce.purchase.DisneyPlusCommerceCreditCardTest">
                <methods>
                    <exclude name="annual"/>
                    <exclude name="paymentBlocked"/>
                    <exclude name="error"/>
                    <exclude name="deepLink"/>
                    <exclude name="VermontMonthly"/>
                    <exclude name="superBundleIneligible"/>
                    <exclude name="superBundleRedirect"/>
                    <exclude name="superBundleRedirectPage"/>
                </methods>
            </class>
        </classes>
    </test>

