<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="ios_tablet_ratings_branch" annotations="JDK" configfailurepolicy="continue" thread-count="8">
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise,Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_tablet_ratings_branch"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPad_8_Gen_1,iPad_Mini_5_Gen,iPad_mini_4,iPad_Air_2,iPad_Air_4_Gen,iPad_5_Gen,iPad_Gen_9,iPad_Pro_2,iPad_Pro_Gen_3;deviceType=Tablet;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::groups::Test Group selection" value="NonUS-Ratings"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=480,init_retry_count=2,init_retry_interval=15"/>

    <test verbose="1" name="DisneyPlus iOS Tablet APAC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsAPACTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet KCC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsKCCTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet KMRB Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsKMRBTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet R21 Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingR21Test"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Turkey AI Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingTurkeyAITest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet New Zealand OFLC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingNewZealandOFLCTest"/>
        </classes>
    </test>
</suite>
