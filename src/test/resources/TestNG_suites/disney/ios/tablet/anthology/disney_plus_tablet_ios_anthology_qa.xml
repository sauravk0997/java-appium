<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" thread-count="8" name="iOS - Disney+ - Tablet Anthology QA" annotations="JDK">

    <parameter name="suiteOwner" value="csolmaz"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Enterprise,Dominguez-Non-IAP-QA-Enterprise-for-Automation,"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="DisneyPlus-iOS-Tablet-Anthology-QA"/>
    <parameter name="jenkinsEmail"
               value="mobileqa@disneystreaming.com,eric.liao@disney.com,Lauren.Churus@disneystreaming.com,jennifer.yee@disneystreaming.com,katie.hampton@disneystreaming.com,sydnee.stanley@disneystreaming.com,bryce.horwood@disneystreaming.com"/>
    <parameter name="jenkinsEnvironments" value="QA"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=ANY;deviceType=Tablet;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="1"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsRegressionPipeline" value="iOS-Regression-Tablet-Anthology-QA"/>
    <parameter name="jenkinsRegressionScheduling" value="H 12,13,14,15 * * *"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="overrideFields" value=""/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression." value=",Anthology"/>

    <test verbose="1" name="DisneyPlus iOS Tablet Anthology QA" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.anthology.DisneyPlusAnthologyTest"/>
        </classes>
    </test>
</suite>