<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="dmgzios_tablet_smoke" annotations="JDK" configfailurepolicy="continue" thread-count="3" parallel="tests">
    <parameter name="suiteOwner" value="apple-automation"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="jenkinsJobName" value="dmgzios_tablet_smoke"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="jenkinsRegressionPipeline" value="iOS-Regression-Tablet-Pipeline"/>
    <parameter name="reporting.notification.slack-channels" value="cse-apple-zebrunner-reports"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::reporting.milestone.name::Enter Milestone Name" value=""/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="jenkinsBuild" value=""/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Non-IAP-PreProd-Enterprise"/>
    <parameter name="jenkinsBuild" value="3.9.2.73980.3"/>
    <parameter name="jenkinsEnvironments" value="PreProd"/>
    <parameter name="hiddenParam::groups::group Name" value="HuluHub"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPad_8_Gen_1,iPad_8_Gen,iPad_Mini_5_Gen,iPad_Pro_Gen_3_1,iPad_Gen_9;deviceType=Tablet;timeoutMs=120000;appPushTimeout=90000"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="jenkinsEmail" value="eric.liao@disney.com,vaibhavi.zaveri@disney.com"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=300,capabilities.newCommandTimeout=240,init_retry_count=2,init_retry_interval=15"/>

    <test verbose="1" name="DisneyPlus iOS Tablet Details" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.details.DisneyPlusDetailsSeriesTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.details.DisneyPlusDetailsMovieTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Search" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.search.DisneyPlusSearchTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Home" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.home.DisneyPlusHomeTest"/>
        </classes>
    </test>
</suite>
