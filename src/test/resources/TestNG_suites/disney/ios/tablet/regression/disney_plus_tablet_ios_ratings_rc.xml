<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="ios_tablet_ratings_rc" annotations="JDK" configfailurepolicy="continue" thread-count="8">
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="hiddenParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_tablet_ratings_rc"/>
    <parameter name="jenkinsEmail" value="Katie.Hampton@disney.com,Sydnee.Stanley@disney.com,Eric.Appel@disney.com,eric.liao@disney.com,jared.e.watterworth@disney.com,vaibhavi.zaveri@disney.com"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPad_Mini_5_Gen,iPad_mini_4,iPad_Air_4_Gen,iPad_Gen_9,iPad_Pro_2,iPad_Pro_Gen_3;deviceType=Tablet;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=480,init_retry_count=2,init_retry_interval=15"/>

    <test verbose="1" name="DisneyPlus iOS Tablet APAC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusAPACRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet KCC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusKCCRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet KMRB Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusKMRBRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet AI Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusAIRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet OFLC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusOFLCRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet DJCTQ Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusBrazilDJCTQRatingsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet EMEA Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusEMEARatingsTest"/>
        </classes>
