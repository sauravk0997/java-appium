<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="ios_tablet_regression_dev" annotations="JDK" configfailurepolicy="continue" thread-count="100" parallel="tests">

    <parameter name="suiteOwner" value="gkrishna1"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name"
               value="Disney-Prod-Enterprise,Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_tablet_regression_dev"/>
    <parameter name="jenkinsEmail"
               value="eric.liao@disney.com,jared.e.watterworth@disney.com"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities"
               value="platformName=iOS;deviceName=iPad_Mini_5_Gen,iPad_mini_4,iPad_Air_2,iPad_Pro_2,iPad_5_Gen,iPad_Gen_9;deviceType=Tablet;timeoutMs=120000;appPushTimeout=90000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="jenkinsRegressionPipeline" value="iOS-Regression-Tablet-Pipeline"/>
    <parameter name="scheduling" value="H 7 * * *"/>
    <parameter name="jenkinsBuild" value="3.2.0.67481"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=480,init_retry_count=2,init_retry_interval=15"/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression."
               value=",Ariel,Ariel-More Menu,Details,Home,Hulk,More Menu,NonUS-More Menu,Onboarding,Search,Smoke,Video Player"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value="XMOBQA-75493"/>

    <test verbose="1" name="DisneyPlus iOS Tablet Basic Acceptance" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.basic.DisneyPlusBasicTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet Onboarding1" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.onboarding.DisneyPlusLoginTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.onboarding.DisneyPlusWelcomeScreenTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.onboarding.DisneyPlusWelcomeScreenTabletTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet Onboarding2" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.onboarding.DisneyPlusSignUpTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.onboarding.DisneyPlusForgotPasswordTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet More Menu1" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuSettingsTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuAppSettingsTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuWatchlistTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet More Menu2" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuAccountSettingsTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuProfilesTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuLegalTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet More Menu3" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusMoreMenuArielProfilesTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Handset More Menu Non US" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.moremenu.DisneyPlusNonUSMoreMenuProfilesTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet Details Acceptance" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.details.DisneyPlusDetailsTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.details.DisneyPlusDetailsSeriesTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.details.DisneyPlusDetailsMovieTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet Video Player Acceptance" parallel="methods">
        <parameter name="iOS17TabletDevices"
                   value="iPad_Mini_5_Gen,iPad_Gen_9,iPad_Air_4_Gen,iPad_8_Gen_1"/>
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.videoplayer.DisneyPlusVideoAudioSubtitlesMenuTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.videoplayer.DisneyPlusVideoPlayerControlTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.videoplayer.DisneyPlusVideoUpNextTest"/>
        </classes>
    </test>

    <test verbose="1" name="DisneyPlus iOS Tablet Search" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.search.DisneyPlusSearchTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Home" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.home.DisneyPlusHomeTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.home.DisneyPlusBrandsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Hulk" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkHomeTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkSearchTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkDetailsTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkProfilesTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Hulk2" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkWatchlistTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkNetworkPageTest"/>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkDownloadsTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Tablet Hulk2 Video Player" parallel="methods">
        <parameter name="iOS17TabletDevices"
                   value="iPad_Mini_5_Gen,iPad_Gen_9,iPad_Air_4_Gen,iPad_8_Gen_1"/>
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkVideoPlayerTest"/>
        </classes>
    </test>
</suite>
