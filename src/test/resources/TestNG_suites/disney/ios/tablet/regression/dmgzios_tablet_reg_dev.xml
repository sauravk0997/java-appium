    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="stringParam::reporting.milestone.name::Enter Milestone Name" value="apple-3.7.0-fc"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value="XMOBQA-77149"/>
    <parameter name="jenkinsBuild" value="3.7.0.72330"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise,Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="dmgzios_tablet_reg_dev"/>
    <parameter name="jenkinsEmail" value="eric.liao@disney.com,vaibhavi.zaveri@disney.com"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPad_Mini_5_Gen,iPad_Air_4_Gen,iPad_Gen_9,iPad_Pro_Gen_3,iPad_8_Gen_1;deviceType=Tablet;timeoutMs=120000;appPushTimeout=90000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
