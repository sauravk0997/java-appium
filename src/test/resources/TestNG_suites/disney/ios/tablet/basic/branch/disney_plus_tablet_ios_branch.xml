<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" thread-count="7" name="ios_tablet_branch" annotations="JDK" configfailurepolicy="continue" parallel="methods">

    <parameter name="suiteOwner" value="csolmaz"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Dominguez-Non-IAP-Prod-Enterprise-for-Automation,Disney-Prod-Enterprise"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_tablet_branch"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities"
               value="platformName=iOS;deviceName=ANY;deviceType=Tablet;timeoutMs=120000;appPushTimeout=90000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="jenkinsJobExecutionOrder" value="0"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="booleanParam::enableOneTrustConfig::by Default enabled to fix 5 tests, uncheck to disable oneTrustConfig." value="true"/>
    <parameter name="overrideFields" value="enableOneTrustConfig=${enableOneTrustConfig},JOB_MAX_RUN_TIME=360,appVersion=${appVersion},init_retry_count=2,init_retry_interval=15"/>
    <parameter name="stringParam::appVersion::input app version for oneTrustConfig" value=""/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression."
               value=",Alice,Ariel,Ariel-More Menu,BVT,Smoke,Hulk,Onboarding,More Menu,Details,Video Player,Search"/>

