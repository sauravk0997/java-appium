<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="ios_tablet_hulk_upload" annotations="JDK" configfailurepolicy="continue" thread-count="1" parallel="tests">

    <parameter name="suiteOwner" value="csolmaz"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise,Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_tablet_upload"/>
    <parameter name="jenkinsEnvironments" value="PROD"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPad_8_Gen_1;deviceType=Tablet;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="1"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=480,init_retry_count=2,init_retry_interval=15"/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression."
               value="Hulk-Upload"/>

    <test verbose="1" name="iOS Tablet Hulk Continue Watching Compare" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkContinueWatchingUploadTest"/>
        </classes>
    </test>
    <test verbose="1" name="iOS Tablet Hulk Disney Five Brand Upload" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.Hulk.DisneyPlusHulkDisneyFiveBrandUploadTest"/>
        </classes>
    </test>
</suite>
