<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" thread-count="12" name="ios_handset_anthology_prod" annotations="JDK" configfailurepolicy="continue">

    <parameter name="suiteOwner" value="csolmaz"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="choiceParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise,Dominguez-Non-IAP-Prod-Enterprise-for-Automation"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_handset_anthology_prod"/>
    <parameter name="jenkinsEmail"
               value="mobileqa@disney.com,eric.liao@disney.com,Lauren.Churus@disney.com,katie.hampton@disney.com,sydnee.stanley@disney.com,bryce.horwood@disney.com,jared.e.watterworth@disney.com"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=ANY;deviceType=Phone;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="1"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsRegressionPipeline" value="iOS-Regression-Handset-Anthology"/>
    <parameter name="scheduling" value="H/55 12 * * 1,2::H/55 0,1 * * 2,3"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="overrideFields" value="disableFlexWelcomeConfig=${disableFlexWelcomeConfig},init_retry_count=2,init_retry_interval=15,retry_count=1"/>
    <parameter name="booleanParam::disableFlexWelcomeConfig::by Default disabled uncheck to enable Flex welcome screen." value="true"/>
    <parameter name="choiceParam::groups::Test Group selection. Leave blank to run full regression." value=",Anthology"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="jenkinsBuild" value="2.23.0.59750"/>

    <test verbose="1" name="DisneyPlus iOS Handset Anthology" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.anthology.DisneyPlusAnthologyTest"/>
        </classes>
    </test>
</suite>