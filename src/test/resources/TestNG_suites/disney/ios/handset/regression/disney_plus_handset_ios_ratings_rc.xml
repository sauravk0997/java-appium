<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="ios_handset_ratings_rc" annotations="JDK" configfailurepolicy="continue" thread-count="10">
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="hiddenParam::appcenter_app_name::app center app name" value="Disney-Prod-Enterprise"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobName" value="ios_handset_ratings_rc"/>
    <parameter name="jenkinsEmail" value="Katie.Hampton@disney.com,Sydnee.Stanley@disney.com,Eric.Appel@disney.com,eric.liao@disney.com,jared.e.watterworth@disney.com,vaibhavi.zaveri@disney.com"/>
    <parameter name="jenkinsEnvironments" value="Prod"/>
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPhone_8,iPhone_8Plus,iPhone_Xr,iPhone_Xs,iPhone_11,iPhone_11_1,iPhone_11_2,iPhone_11_3,iPhone_12,iPhone_13_Pro,iPhone_14;deviceType=Phone;timeoutMs=120000"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="reporting.notification.slack-channels" value="ios-tvos-test-automation"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=360,init_retry_count=2,init_retry_interval=15"/>

    <test verbose="1" name="DisneyPlus iOS Handset APAC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsAPACTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Handset KCC Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsKCCTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Handset KMRB Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingsKMRBTest"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Handset R21 Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingR21Test"/>
        </classes>
    </test>
    <test verbose="1" name="DisneyPlus iOS Handset Turkey AI Ratings">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.ratings.DisneyPlusNonUSRatingTurkeyAITest"/>
        </classes>
    </test>
</suite>
