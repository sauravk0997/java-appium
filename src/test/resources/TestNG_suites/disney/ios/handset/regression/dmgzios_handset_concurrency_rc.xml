<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="dmgzios_handset_concurrency_rc" annotations="JDK" configfailurepolicy="continue"
       thread-count="6"
       parallel="tests">
    <parameter name="suiteOwner" value="apple-automation"/>
    <parameter name="reporting.project-key" value="DIS"/>
    <parameter name="jenkinsJobName" value="dmgzios_handset_concurrency"/>
    <parameter name="jenkinsJobType" value="ios"/>
    <parameter name="jenkinsJobExecutionOrder" value="1"/>
    <parameter name="jenkinsJobExecutionMode" value="continue"/>
    <parameter name="jenkinsRegressionPipeline" value="iOS-Regression-Handset-Pipeline"/>
    <!--    <parameter name="scheduling" value="H 7 * * 1-5"/>-->
    <parameter name="jenkinsFetchAppFrom" value="appcenter"/>
    <parameter name="provider" value="mcloud"/>
    <parameter name="jenkinsDefaultRetryCount" value="0"/>
    <parameter name="stringParam::reporting.milestone.name::Enter Milestone Name" value=""/>
    <parameter name="stringParam::reporting.tcm.xray.test-execution-key::Enter Xray Execution value" value=""/>
    <parameter name="choiceParam::appcenter_app_name::app center app name"
               value="Disney-Prod-Enterprise,Disney-Non-IAP-PreProd-Enterprise"/>
    <parameter name="jenkinsBuild" value="4.0.0.74899"/>
    <parameter name="jenkinsEnvironments" value="Prod,PreProd"/>
    <parameter name="hiddenParam::groups::group Name" value="Concurrency"/>
    <parameter name="capabilities" value="platformName=iOS;deviceName=iPhone_14,iPhone_12,iPhone_12_Pro,iPhone_Xs,iPhone_13_Pro,iPhone_11_2;deviceType=Phone;timeoutMs=120000;appPushTimeout=90000"/>
    <parameter name="stringParam::test::testclass#testmethod filer. If empty execute all of them" value=""/>
    <parameter name="overrideFields" value="JOB_MAX_RUN_TIME=300,capabilities.newCommandTimeout=240,init_retry_count=2,init_retry_interval=15"/>
    <parameter name="stringParam::accountEmail::Account email to be used" value="defaultEmailValue"/>
    <parameter name="stringParam::accountPassword::Account password to be used" value="defaultPasswordValue"/>

    <test verbose="1" name="DisneyPlus iOS Handset Concurrency" parallel="methods">
        <classes>
            <class name="com.disney.qa.tests.disney.apple.ios.regression.concurrency.DisneyPlusConcurrencyTest"/>
        </classes>
    </test>
</suite>
