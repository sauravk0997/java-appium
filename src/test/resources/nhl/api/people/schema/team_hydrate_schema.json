{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "default": 5
    },
    "name": {
      "type": "string",
      "default": "Pittsburgh Penguins"
    },
    "link": {
      "type": "string",
      "default": "/api/v1/teams/5"
    },
    "venue": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "default": "CONSOL Energy Center"
        },
        "link": {
          "type": "string",
          "default": "/api/v1/venues/null"
        },
        "city": {
          "type": "string",
          "default": "Pittsburgh"
        },
        "timeZone": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "default": "America/New_York"
            },
            "offset": {
              "type": "integer",
              "default": -4
            },
            "tz": {
              "type": "string",
              "default": "EDT"
            }
          },
          "required": [
            "id",
            "offset",
            "tz"
          ]
        }
      },
      "required": [
        "name",
        "link",
        "city",
        "timeZone"
      ]
    },
    "abbreviation": {
      "type": "string",
      "default": "PIT"
    },
    "teamName": {
      "type": "string",
      "default": "Penguins"
    },
    "locationName": {
      "type": "string",
      "default": "Pittsburgh"
    },
    "firstYearOfPlay": {
      "type": "string",
      "default": "1967"
    },
    "division": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "default": 18
        },
        "name": {
          "type": "string",
          "default": "Metropolitan"
        },
        "link": {
          "type": "string",
          "default": "/api/v1/divisions/18"
        }
      },
      "required": [
        "id",
        "name",
        "link"
      ]
    },
    "conference": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "default": 6
        },
        "name": {
          "type": "string",
          "default": "Eastern"
        },
        "link": {
          "type": "string",
          "default": "/api/v1/conferences/6"
        }
      },
      "required": [
        "id",
        "name",
        "link"
      ]
    },
    "franchise": {
      "type": "object",
      "properties": {
        "franchiseId": {
          "type": "integer",
          "default": 17
        },
        "teamName": {
          "type": "string",
          "default": "Penguins"
        },
        "link": {
          "type": "string",
          "default": "/api/v1/franchises/17"
        }
      },
      "required": [
        "franchiseId",
        "teamName",
        "link"
      ]
    },
    "shortName": {
      "type": "string",
      "default": "Pittsburgh"
    },
    "officialSiteUrl": {
      "type": "string",
      "default": "http://www.pittsburghpenguins.com"
    },
    "franchiseId": {
      "type": "integer",
      "default": 17
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  },
  "required": [
    "id",
    "name",
    "link",
    "venue",
    "abbreviation",
    "teamName",
    "locationName",
    "firstYearOfPlay",
    "division",
    "conference",
    "franchise",
    "shortName",
    "officialSiteUrl",
    "franchiseId",
    "active"
  ]
}