{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "playerLongBio": {
      "type": "object",
      "properties": {
        "contentId": {
          "type": "integer"
        },
        "bioArticleLink": {
          "type": "string"
        },
        "fullBio": {
          "type": "string"
        }
      },
      "required": [
        "contentId",
        "bioArticleLink",
        "fullBio"
      ]
    },
    "playerShortBio": {
      "type": "object",
      "properties": {
        "contentId": {
          "type": "integer"
        },
        "shortBio": {
          "type": "string"
        }
      },
      "required": [
        "contentId",
        "shortBio"
      ]
    }
  },
  "required": [
    "playerLongBio",
    "playerShortBio"
  ]
}